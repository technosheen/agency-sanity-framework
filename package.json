{
  "name": "mawla-engine-v2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run build-config && next dev",
    "lint": "next lint && tsc --noEmit",
    "build": "npm run build-config && if test \"$VERCEL_GIT_REPO_SLUG\" = \"mawla-engine-v2\" ; then npm run build-and-test ; else npm run build-only; fi",
    "build-only": "npm run build-config && next build && npm run build-cms",
    "build-and-test": "npm run build-config && npm test && next build && npm run build-cms",
    "start": "npm run build-config && next start",
    "cms": "sanity dev",
    "build-cms": "sanity build public/cms -y",
    "storybook": "npm run build-config && storybook dev -p 6006 --no-open",
    "build-storybook": "npm run build-config && storybook build -o public/storybook --disable-telemetry",
    "create-page": "sanity exec ./cli/create-page -- --write",
    "create-block": "sanity exec ./cli/create-block -- --write",
    "create-module": "echo \"Modules are now called blocks. Use yarn create-block instead.\"",
    "build-config": "sanity exec ./cli/config/build.ts -- --write",
    "test-cli": "NODE_ENV=test npx jest ./cli/ ",
    "test": "NODE_ENV=test TZ=UTC jest",
    "test-watch": "jest --watch"
  },
